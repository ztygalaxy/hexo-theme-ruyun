<style type="text/css">
    header.intro-header{
        background-position: right; 
    }
</style>
<!-- Chinese Version -->
<div class="zh post-container">
	<%if (page.musicid){%>
		<!-- cloud music -->
		<!-- 单曲嵌入， auto=1 可以控制自动播放与否，当值为 1 即打开网页就自动播放，值为 0 时需要访客手动点击播放 -->
		<iframe frameborder="no" border="0" marginwidth="0" marginheight="0" width=100% height=86		src="//music.163.com/outchain/player?type=2&id=<%=page.musicid%>&auto=<%=page.musicauto||1%>&height=66">
		</iframe>
	<% } %>
    <%- page.content %>
	<%- page.musicid %>
</div>

